#include "libft.h"
#include <stdio.h>  // printf
#include <string.h> // strdup
#include <ctype.h>  // toupper/tolower
 
char	ft_changelower(char c)
{
	return (ft_tolower(c));
}
 
int	main(void)
{
	char str[] = "hElLoWoRlD";
	char *arr;
	arr = ft_strmap(str, &ft_changelower);
 
	printf("%s\n", arr);
	return (0);
}
